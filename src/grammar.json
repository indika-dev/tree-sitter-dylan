{
  "name": "dylan",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "meta_preprocessor_dylan"
    },
    "comment_block_dylan": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "///"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "meta_preprocessor_dylan": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "module"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "synopsis"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "author"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "copyright"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "version"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "files"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "executable"
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "library"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        }
      ]
    },
    "module": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "module"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "synopsis": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "synopsis"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "author": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "author"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "copyright": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "copyright"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "string_literal"
            },
            {
              "type": "SYMBOL",
              "name": "number"
            }
          ]
        }
      ]
    },
    "version": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "version"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "files": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "files"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "executable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "executable"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "library": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "library"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "character_literal"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "STRING",
          "value": "#t"
        },
        {
          "type": "STRING",
          "value": "#f"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#("
            },
            {
              "type": "SYMBOL",
              "name": "constant"
            },
            {
              "type": "STRING",
              "value": "."
            },
            {
              "type": "SYMBOL",
              "name": "constant"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#("
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "constant"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#["
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "constant"
                  },
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  }
                ]
              }
            },
            {
              "type": "STRING",
              "value": "]"
            }
          ]
        }
      ]
    },
    "string_literal": {
      "type": "PATTERN",
      "value": "[a-zA-Z \\.,0-9]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "character_literal": {
      "type": "PATTERN",
      "value": "[a-z\\-$\\*]"
    },
    "symbol": {
      "type": "STRING",
      "value": "_"
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        }
      ]
    },
    "function_word": {
      "type": "STRING",
      "value": "(none)"
    },
    "hashtag_word": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "#t"
        },
        {
          "type": "STRING",
          "value": "#f"
        },
        {
          "type": "STRING",
          "value": "#next"
        },
        {
          "type": "STRING",
          "value": "#rest"
        },
        {
          "type": "STRING",
          "value": "#key"
        },
        {
          "type": "STRING",
          "value": "#all-keys"
        },
        {
          "type": "STRING",
          "value": "#include"
        }
      ]
    },
    "begin_word": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "begin"
        },
        {
          "type": "STRING",
          "value": "block"
        },
        {
          "type": "STRING",
          "value": "case"
        },
        {
          "type": "STRING",
          "value": "for"
        },
        {
          "type": "STRING",
          "value": "if"
        },
        {
          "type": "STRING",
          "value": "method"
        },
        {
          "type": "STRING",
          "value": "select"
        },
        {
          "type": "STRING",
          "value": "unless"
        },
        {
          "type": "STRING",
          "value": "until"
        },
        {
          "type": "STRING",
          "value": "while"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
